<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sorry.dao.CrawlerDAO">
    <insert id="insert" parameterType="CrawlerVO">
        INSERT INTO cralwerData(
             rcpNo
            ,dcmNo
            ,offset_no
            ,length_no
        )values(
            #{rcpNo}
           ,#{dcmNo}
           ,#{offset_no}
           ,#{length_no}
        )
    </insert>    
</mapper>  









